CREATE TABLE curtir (
cod_postagem int,
cod_usuario int,
dathora_curtir timestamp NOT NULL,
FOREIGN KEY(cod_postagem) REFERENCES postagem (cod_postagem),
FOREIGN KEY(cod_usuario) REFERENCES usuario (cod_usuario)
);
INSERT INTO curtir(dathora_curtir) VALUES (1970-01-01 13:40:10),(1999-06-05 15:20:22), (2005-08-01 23:10:44), (1980-06-07 12:12:30), (1997-01-05 14:25:11),( 1990-05-08 11:40:20), (2020-09-01 16:15:45), (1980-08-06 18:35:30), (1977-07-07 04:20:15), (1983-03-08 12:12:39);

