CREATE TABLE doacao (
cod_ong int,
cod_usuario int,
dt_doacao date NOT NULL,
valor_doacao decimal(10,2) NOT NULL,
FOREIGN KEY(cod_ong) REFERENCES ong (cod_ong),
FOREIGN KEY(cod_usuario) REFERENCES usuario (cod_usuario)
);
