CREATE TABLE ong (
email_ong varchar(250) NOT NULL UNIQUE,
telefone_ong varchar(14) NOT NULL,
nome_ong varchar(100) NOT NULL,
cod_ong int AUTO_INCREMENT PRIMARY KEY
);
